{
  "name": "Telefone",
  "plural": "telefones",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "DisableAllMethods": {
      "expose": [
        "findById"
      ]
    }
  },
  "scope": {
    "include": {
      "relation": "telefoneTipo"
    }
  },
  "properties": {
    "ddi": {
      "type": "string"
    },
    "ddd": {
      "type": "string"
    },
    "numero": {
      "type": "string",
      "required": true
    },
    "whatsapp": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "telefoneTipo": {
      "type": "belongsTo",
      "model": "TelefoneTipo",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
