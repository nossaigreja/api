{
  "name": "Endereco",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "DisableAllMethods": {
      "expose": []
    }
  },
  "scope": {
    "include": {
      "relation": "enderecoTipo"
    }
  },
  "properties": {
    "cep": {
      "type": "string",
      "required": true
    },
    "logradouro": {
      "type": "string",
      "required": true
    },
    "bairro": {
      "type": "string"
    },
    "complemento": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "enderecoTipo": {
      "type": "belongsTo",
      "model": "EnderecoTipo",
      "foreignKey": ""
    },
    "municipio": {
      "type": "belongsTo",
      "model": "Municipio",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
