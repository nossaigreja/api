{
  "name": "Usuario",
  "plural": "usuarios",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "DisableAllMethods": {
      "expose": [
        "findById",
        "find",
        "create",
        "updateAttributes",
        "exists",
        "confirm",
        "count",
        "login",
        "logout",
        "resetPassword",
        "updatePassword",
        "upload",
        "download"
      ]
    }
  },
  "scope": {
    "include": [
      {
        "relation": "enderecos"
      },
      {
        "relation": "telefones"
      }
    ]
  },
  "properties": {
    "imagem": {
      "type": "string"
    },
    "nome": {
      "type": "string",
      "required": true
    },
    "dtNascimento": {
      "type": "date"
    },
    "sexo": {
      "type": "string"
    },
    "dtCriacao": {
      "type": "date",
      "required": true
    },
    "dtAlteracao": {
      "type": "date"
    },
    "dtRemocao": {
      "type": "date"
    },
    "pai": {
      "type": "string"
    },
    "mae": {
      "type": "string"
    },
    "rg": {
      "type": "string"
    },
    "rgOrgao": {
      "type": "string"
    },
    "dtCasamento": {
      "type": "date"
    },
    "observacao": {
      "type": "string"
    },
    "pne": {
      "type": "boolean",
      "default": false
    },
    "cpf": {
      "type": "string"
    },
    "membrado": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "filial": {
      "type": "belongsTo",
      "model": "Filial",
      "foreignKey": ""
    },
    "enderecos": {
      "type": "hasAndBelongsToMany",
      "model": "Endereco",
      "foreignKey": ""
    },
    "telefones": {
      "type": "hasAndBelongsToMany",
      "model": "Telefone",
      "foreignKey": ""
    },
    "usuarioAcessos": {
      "type": "hasMany",
      "model": "UsuarioAcesso",
      "foreignKey": ""
    },
    "naturalidade": {
      "type": "belongsTo",
      "model": "Municipio",
      "foreignKey": "naturalidadeId"
    },
    "conjuge": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": "conjugeId"
    },
    "estadoCivil": {
      "type": "belongsTo",
      "model": "EstadoCivil",
      "foreignKey": ""
    },
    "profissao": {
      "type": "belongsTo",
      "model": "Cbo",
      "foreignKey": "profissaoId"
    },
    "escolaridade": {
      "type": "belongsTo",
      "model": "Escolaridade",
      "foreignKey": ""
    },
    "midiaSocials": {
      "type": "hasAndBelongsToMany",
      "model": "MidiaSocial",
      "foreignKey": ""
    },
    "motivoEntrada": {
      "type": "belongsTo",
      "model": "MotivoEntrada",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "updatePassword"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "upload"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "download"
    }
  ],
  "methods": {}
}
